{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning",
      "FomMon": "Debug"
    }
  },
  "ConnectionStrings": {
    "application": "Host=postgres;Database=application;Username=postgres;Password="
  },
  "WfsDownloader": {
    "TimeoutSeconds": 360,
    "Ogr2OgrPath": "ogr2ogr",
    "OgrWfsPageSize": 10000,
    "OgrWfsPagingAllowed": true,
    "AcceptZeroFeaturesAfterAttempts": 3
  },
  "Testing": {
    "IncomingDelay": {
      "FromMs": 200,
      "ToMs": 1000
    }
  }
}
