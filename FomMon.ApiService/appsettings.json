{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning",
      "FomMon.ApiService.Infrastructure.DbPolicy": "Debug"
    }
  },
  "AllowedHosts": "*",

  "WfsDownloader": {
    "TimeoutSeconds": 360,
    "Ogr2OgrPath": "ogr2ogr",
    "OgrWfsPageSize": 10000,
    "OgrWfsPagingAllowed": true,
    "AcceptZeroFeaturesAfterAttempts": 3
  }
}
