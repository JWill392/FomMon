{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning",
      "FomMon.ApiService.Infrastructure.DbPolicy": "Debug"
    }
  },
  "FomDownloader": {
    "RefreshProjectInterval": "12:00:00",
    "ThrottleApiDuration": "00:00:01.0"
  },
  "FomPollingService": {
    "PollingInterval": "01:00:00"
  },
  "AllowedHosts": "*",

  "WfsDownloader": {
    "TimeoutSeconds": 360,
    "Ogr2OgrPath": "ogr2ogr",
    "OgrWfsPageSize": 10000,
    "OgrWfsPagingAllowed": true
  }
}
